
#include <iostream>

using namespace std;

int main()
{
    int sheluji[3];
    
    cout<<"enter the value of an array"<<endl;
    
    for (int i=0; i<3;i++){
        cin>>sheluji[i];
        
    }
   
    cout<<"these values are entred"<<endl;
    
    for (int i=0; i<3; i++){
        cout<<sheluji[i]<<endl;
    }
    
    return 0;
}
